@{
    ViewBag.Title = "Home Page";
}

<div class="container">
    <h3>2021 Formula One Liveries</h3>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
            <li data-target="#myCarousel" data-slide-to="4"></li>
            <li data-target="#myCarousel" data-slide-to="5"></li>
            <li data-target="#myCarousel" data-slide-to="6"></li>
            <li data-target="#myCarousel" data-slide-to="7"></li>
            <li data-target="#myCarousel" data-slide-to="8"></li>
            <li data-target="#myCarousel" data-slide-to="9"></li>
        </ol>

        <div class="carousel-inner">
            <div class="item active">
                <img src="/content/images/alfa-romeo-c41.jpg" alt="Alfa Romeo C41" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/alphatauri-at02.jpg" alt="AlphaTauri AT02" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/alpine-a521.jpg" alt="Alpine A521" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/aston-martin-amr21.jpg" alt="Aston Martin AMR21" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/ferrari-sf21.jpg" alt="Ferrari SF21" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/haas-vf21jpg.jpg" alt="Haas VF21" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/mclaren-mcl35m.jpg" alt="Mclaren MCL35m" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/mercedes-amg-f1-w12.jpg" alt="Mercedes W12" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/red-bull-rb16b.jpg" alt="Red Bull RB16B" style="width:100%;">
            </div>
            <div class="item">
                <img src="/content/images/williams-fw43b.jpg" alt="Williams FW43B" style="width:100%;">
            </div>
        </div>

        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<br />

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h4>The carousel above shows all 10 teams F1 liveries for the 2021 season.</h4>
        </div>

    </div>

</div>
